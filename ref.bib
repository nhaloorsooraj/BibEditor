@article{moon_ultra-low_2012,
	title = {Ultra-low resistance ohmic contacts in graphene field effect transistors},
	volume = {100},
	issn = {0003-6951, 1077-3118},
	url = {https://pubs.aip.org/apl/article/100/20/203512/25403/Ultra-low-resistance-ohmic-contacts-in-graphene},
